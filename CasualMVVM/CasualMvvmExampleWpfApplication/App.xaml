<Application x:Class="CasualMvvmExampleWpfApplication.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CasualMvvmExampleWpfApplication"
             xmlns:view="clr-namespace:CasualMvvmExampleWpfApplication" 
             xmlns:pages="clr-namespace:FuchsiaSoft.CasualMvvmExampleWpfApplication.Pages"
             xmlns:viewmodel="clr-namespace:FuchsiaSoft.CasualMvvmExampleWpfApplication.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
        <!--DataTemplates for viewmodel to view associations-->
        <DataTemplate DataType="{x:Type viewmodel:ChildWindowViewModel}">
            <pages:ChildPage/>
        </DataTemplate>
        
    </Application.Resources>
</Application>
